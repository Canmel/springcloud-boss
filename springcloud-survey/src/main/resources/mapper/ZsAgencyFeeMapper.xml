<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.camel.survey.mapper.ZsAgencyFeeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.camel.survey.model.ZsAgencyFee">
        <id column="id" property="id" />
        <result column="username" property="username" />
        <result column="phone" property="phone" />
        <result column="id_num" property="idNum" />
        <result column="salary" property="salary" />
        <result column="status" property="status" typeHandler="com.camel.survey.enumhandler.MyIntegerToEnumHandler"/>
        <result column="gain" property="gain" />
        <result column="gain_time" property="gainTime" />
        <result column="work_id" property="workId" />
        <result column="work_salary" property="workSalary" />
        <result column="work_name" property="workName" />
        <result column="work_hours" property="workHours" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, username, phone, id_num, salary, status, gain, gain_time, work_id, work_salary, work_name, work_hours
    </sql>

    <select id="list" resultMap="BaseResultMap">
        select
         <include refid="Base_Column_List"></include>
        from
        zs_agency_fee
    </select>
</mapper>
