<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.camel.survey.mapper.SysUserMapper">
    <select id="selectByUid" resultType="com.camel.core.model.SysUser">
        select sys_user.uid,sys_user.username,sys_user.company_id,sys_user.email
        from sys_user
        where sys_user.uid = #{uid}
    </select>
</mapper>
