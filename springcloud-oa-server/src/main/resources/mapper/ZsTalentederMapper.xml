<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.camel.oa.mapper.ZsTalentederMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.camel.oa.model.ZsTalenteder">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="cantact_phone" property="cantactPhone" />
        <result column="remark" property="remark" />
        <result column="project_id" property="projectId" />
        <result column="creator" property="creator" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, cantact_phone, remark, project_id, creator, status
    </sql>

    <select id="list" resultMap="BaseResultMap">
        select
         <include refid="Base_Column_List"></include>
        from
        zs_talenteder
    </select>
</mapper>
